{
    "name": "krzysztofzylka/micro-framework",
    "description": "Very fast micro PHP framework",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Krzysztofzylka\\MicroFramework\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Krzysztof Żyłka",
            "email": "krzysztofzylka@yahoo.com"
        }
    ],
    "scripts": {
        "post-install-cmd": [
            "cp src/Console/microframework.php vendor/bin/microframework"
        ],
        "post-update-cmd": [
            "cp src/Console/microframework.php vendor/bin/microframework"
        ]
    },
    "funding": [],
    "require": {
        "php": ">=8.1",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-memcached": "*",
        "krzysztofzylka/database-manager": "^v1.0.27",
        "krzysztofzylka/reflection": "^1.0.0",
        "krzysztofzylka/generator": "^1.0.0",
        "krzysztofzylka/strings": "^1.0.0",
        "krzysztofzylka/request": "^1.0.0",
        "krzysztofzylka/file": "^1.0.0",
        "krzysztofzylka/console": "^1.0.0",
        "krzysztofzylka/env": "^1.0.0",
        "maximebf/debugbar": ">=1.0.0",
        "twig/twig": "^3.7"
    }
}
